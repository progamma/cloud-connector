{
  "name": "{connectorName}",
  "passwordPrivateKey": "%CC_KEY%",
  "remoteServers": ["{applicationUrl1}", "{applicationUrl2}"],
  "remoteUserNames": ["{ideUsername}", "{ideOrganization/IdeUsername}", "{ideUrl@ideUsername}"],
  "remoteConfigurationKey": "00000000-0000-0000-0000-000000000000",
  "connectionOptions": {
  },
  "datamodels": [
    {
      "name": "{datamodelName}",
      "class": "Postgres",
      "APIKey": "00000000-0000-0000-0000-000000000000",
      "connectionOptions": {
        "host": "{hostname}",
        "database": "{databaseName}",
        "user": "{username}",
        "password": "{password}",
        "connectionTimeoutMillis": 30000,
        "max": 100
      }
    },
    {
      "name": "{datamodelName}",
      "class": "SQLServer",
      "APIKey": "00000000-0000-0000-0000-000000000000",
      "connectionOptions": {
        "server": "{hostname}",
        "database": "{databaseName}",
        "user": "{username}",
        "password": "{password}",
        "connectionTimeout": 30000,
        "pool": {
          "max": 100
        },
        "options": {
          "useUTC": false,
          "encrypt": true,
          "trustServerCertificate": true
        }
      }
    },
    {
      "name": "{datamodelName}",
      "class": "Oracle",
      "maxRows": 10000,
      "APIKey": "00000000-0000-0000-0000-000000000000",
      "connectionOptions": {
        "connectString": "{hostname/databaseName}",
        "user": "{username}",
        "password": "{password}",
        "poolMax": 100
      }
    },
    {
      "name": "{datamodelName}",
      "class": "MySQL",
      "APIKey": "00000000-0000-0000-0000-000000000000",
      "connectionOptions": {
        "host": "{hostname}",
        "database": "{databaseName}",
        "user": "{username}",
        "password": "{password}",
        "connectionLimit": 100,
        "connectTimeout": 30000
      }
    },
    {
      "name": "{datamodelName}",
      "class": "ODBC",
      "APIKey": "00000000-0000-0000-0000-000000000000",
      "connectionOptions": {
        "connectionString": "DSN={dsnName};UID={username};PWD={password}",
        "maxSize": 100,
        "connectionTimeout": 30
      }
    }
  ],
  "fileSystems": [
    {
      "name": "myFS1",
      "path": "/Users/John/Desktop",
      "permissions": "rw",
      "whiteListedOrigins": [],
      "APIKey": "00000000-0000-0000-0000-000000000000"
    }
  ],
  "plugins": [
    {
      "name": "{pluginName}",
      "class": "{className}",
      "APIKey": "00000000-0000-0000-0000-000000000000"
    }
  ]
}
